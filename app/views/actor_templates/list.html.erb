<h1>List of all actors</h1>

<table border="1">
  <tr>
    <th>ID</th>
    <th>Name</th>
    <th>Image</th>
    <th>Dob</th>
    <th></th>
  </tr>

  <% Actor.all.each do |a_director| %>
    <tr>
      <td><%= a_director.id %></td>
      <td><%= a_director.name %></td>
      <td>
        <img src="<%= a_director.image %>" width="100">
      </td>
      <td><%= a_director.dob %></td>
      <td>
        <a href="/directors/<%= a_director.id %>">Show details</a>
      </td>
    </tr>
  <% end %>
</table>
