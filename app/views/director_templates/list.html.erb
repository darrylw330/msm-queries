
<table border="1">
  <tr>
    <th>ID</th>
    <th>Name</th>
    <th>Image</th>
    <th>Dob</th>
    <th></th>
  </tr>

<h1>List of all directors</h1>

<% Director.all.each do |a_director| %>
 <tr>
    <td><%= a_director.id %></td>
    <td> <img src="<%= a_director.image %>" 
    </td>
    <td><%= a_director.name %></td>
    <td><%= a_director.dob %></td>
    <td>Show details</td>
  </tr>
<% end %>

 
</table>
